{
  "babelrcRoots": [
    ".",
    "packages/*"
  ],
  "presets": [
    "@babel/env",
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-runtime",
    "react-refresh/babel"
  ]
}